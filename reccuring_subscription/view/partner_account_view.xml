<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_partner_account" model="ir.actions.act_window">
        <field name="name">Partner Account</field>
        <field name="res_model">partner.account</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="partner_account_form" model="ir.ui.view">
        <field name="name">partner account form</field>
        <field name="model">partner.account</field>
        <field name="arch" type="xml">
            <form string="partner_account_form">
                <sheet>
                    <group>
                        <field name="account"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
