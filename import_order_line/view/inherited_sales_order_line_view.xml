<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_order_form" model="ir.ui.view">
        <field name="name">order.view.form.inherit</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form//header//button[@name='action_quotation_send']" position="after">
                <button name="import_order_line_btn" string="Import" type="object"
                context="{'default_orders_id': id}"/>
            </xpath>
        </field>
    </record>
</odoo>